{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build Debug",
            "type": "shell",
            "command": "mingw32-make debug",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "Build Release",
            "type": "shell",
            "command": "mingw32-make release",
            "group": "build"
        },
        {
            "label": "Clean",
            "type": "shell",
            "command": "mingw32-make clean"
        }
    ]
}
